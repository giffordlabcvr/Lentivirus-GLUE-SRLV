alignment AL_TREE_OvineCaprine
 
    add member -w "species = 'SRLV' and source.name = 'ncbi-nuccore-srlv'"

    demote member AL_TREE_SRLV_A  --whereClause "sequence.genotype = 'A' and sequence.source.name = 'ncbi-nuccore-srlv'"
    demote member AL_TREE_SRLV_BC --whereClause "sequence.genotype = 'BC' and sequence.source.name = 'ncbi-nuccore-srlv'"
    demote member AL_TREE_SRLV_E  --whereClause "sequence.genotype = 'E' and sequence.source.name = 'ncbi-nuccore-srlv'"
    exit

alignment AL_TREE_SRLV_A
 
