
  module srlvMaxLikelihoodGenotyper genotype sequence -w "species = 'SRLV'"
  

	<cladeCategory>
	
		<name>subgenus</name>	
		<displayName>Subgenus</displayName>	
		<whereClause>clade_category = 'subgenus'</whereClause>
		<distanceScalingExponent>-4.5</distanceScalingExponent>
		<distanceCutoff>0.5</distanceCutoff>
		<finalCladeCutoff>65.0</finalCladeCutoff>
		
	</cladeCategory>


	<cladeCategory>
	
		<name>subtype</name>	
		<displayName>Subtype</displayName>	
		<whereClause>clade_category = 'subtype'</whereClause>
		<distanceScalingExponent>-4.5</distanceScalingExponent>
		<distanceCutoff>1</distanceCutoff>
		<finalCladeCutoff>65.0</finalCladeCutoff>
		
	</cladeCategory>

